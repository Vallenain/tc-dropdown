            },
            "development": {
              "tsConfig": "projects/ngx-tc/input/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/ngx-tc/input/src/test.ts",
            "tsConfig": "projects/ngx-tc/input/tsconfig.spec.json",
            "karmaConfig": "projects/ngx-tc/input/karma.conf.js"
          }
        }
      }
    },
